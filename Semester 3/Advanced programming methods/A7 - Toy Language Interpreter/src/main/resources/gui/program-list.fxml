<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.ChooseProgramController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <ListView fx:id="programList" prefHeight="308.0" prefWidth="588.0">
      <VBox.margin>
         <Insets bottom="5.0" top="5.0"/>
      </VBox.margin>
      <padding>
         <Insets left="15.0"/>
      </padding>
   </ListView>
   <Button fx:id="selectProgramBtn" mnemonicParsing="false" onMouseClicked="#runProgram" text="Select"/>
</VBox>
